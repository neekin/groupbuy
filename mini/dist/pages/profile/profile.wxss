.profile{background-color:#f9f9f9;padding-bottom:60rpx;height:100vh}.profile-header{background-color:#fff;position:relative;display:flex;flex-direction:column;align-items:center;justify-content:center;margin-bottom:24rpx}.profile-header::after{position:absolute;box-sizing:border-box;content:" ";pointer-events:none;right:0;bottom:0;left:0;border-bottom:1rpx solid #f2f4f6;transform:scaleY(.5)}.card-wapper .van-cell-group{margin:6rpx 24rpx 24rpx;background-color:#fff;border-radius:20rpx;padding:6rpx;box-shadow:0 4rpx 20rpx 0 rgba(212,223,245,.34)}.card-wapper .van-cell-group .card-wapper_van-cell_last{background-color:red}.card-wapper .van-cell-group .card-wapper_van-cell_last .van-cell::after{border-bottom:0}.card-wapper .van-hairline--top-bottom:after{border-width:0}.card-wapper .van-cell-group__title{padding:24rpx 24rpx 6rpx}